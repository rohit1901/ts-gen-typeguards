{
  "compilerOptions": {
    "target": "es6",
    "module": "ESNext",
    "declaration": true,
    "outDir": "dist",
    "moduleResolution": "NodeNext",
    "baseUrl": "./src",
    "paths": {
      "@utils": ["utils/index"],
      "@utils/*": ["utils/*"],
      "@generator": ["generator/index"],
      "@generator/*": ["generator/*"],
    }
  },
  "include": [
    "src/**/*.ts",
  ],
  "start": "node src/index.js",
  "exclude": [
    "node_modules",
    "src/out/"
  ]
}
